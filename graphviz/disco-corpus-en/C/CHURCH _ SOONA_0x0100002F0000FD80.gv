	  1313 [label="You: \"Listen, about that nightclub...\""];
	  1314 [label="You: \"Can I do anything?\""];
	  1315 [label="You: (Say nothing.)"];
	  1316 [label="Untitled hub (04)"];
	  1317 [label="Soona, the Programmer: \"I mean, what sound does the nothing make? How can you even listen to something that doesn't exist?\" She turns to face you, the mainframe throwing shadows on her chin."];
	  1318 [label="Inland Empire: Ghosts, speak to me!"];
	  1319 [label="You: \"No, I'm a master circuit-bender, I know *everything*.\""];
