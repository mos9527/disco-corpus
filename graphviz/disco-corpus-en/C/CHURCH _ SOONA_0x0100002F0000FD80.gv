	  1363 [label="You: \"I brought you the filament.\" (Give her the off-site copy.)"];
	  1364 [label="Soona, the Programmer: \"Lintel was able to divine the location of the anomaly from this broken copy. I want to repeat their calculation, only this time with better equipment.\""];
	  1365 [label="You: \"Something's very wrong with that filament memory.\""];
	  1366 [label="Soona, the Programmer: Variable[\"church.soona_logic_excited_about_waterbowls\"]"];
